t\t\t\t\t\t</th>\r\n\t\t\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t\t\t</thead>\r\n\t\t\t\t\t\t\t\t<tbody>\r\n\t\t\t\t\t\t\t\t\t<tr v-for=\"row in paginatedData\">\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"width:10%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.modifiedOn ? new Date(row.modifiedOn).toLocaleString() : ''}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"width:15%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.changeAuthor ? `${row.changeAuthor.name}${row.changeAuthor.role ? ', '+row.changeAuthor.role.name : ''}` : '&mdash;'}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"width:10%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.number}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"width:10%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.stage ? row.stage.name : '&mdash;'}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"text-align:center;width:10%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.status ? row.status.name : '&mdash;'}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t\t<td style=\"width:15%\">\r\n\t\t\t\t\t\t\t\t\t\t\t{{row.name}}\r\n\t\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t\t\t</tbody>\r\n\t\t\t\t\t\t\t</table>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"b-wrapper-page__bottom\">\r\n\t\t\t\t\t<div class=\"b-pagination\">\r\n\t\t\t\t\t\t<ul class=\"b-pagination__list\">\r\n\t\t\t\t\t\t\t<li class=\"b-pagination__item-prev\">\r\n\t\t\t\t\t\t\t\t<a @click=\"prevPage(clickFilter)\" :class=\"{'b-button': true, 'b-button--slider': true, 'b-button--disabled':pageNumber==0 }\" href=\"javascript:void(0);\" title=\"\">\r\n\t\t\t\t\t\t\t\t\t\t<iconAngleleftSvg class=\"b-icon icon-angle-left\"/>\r\n\t\t\t\t\t\t\t\t</a>\r\n\t\t\t\t\t\t\t</li>\r\n\r\n\t\t\t\t\t\t\t\t<li class=\"b-pagination__item\">\r\n\t\t\t\t\t\t\t\t\t\t<span>{{resultCount > 0 ? pageNumber+1 : 0}} из {{resultCount}} стр</span>\r\n\t\t\t\t\t\t\t\t</li>\r\n\r\n\t\t\t\t\t\t\t<li class=\"b-pagination__item-next\">\r\n\t\t\t\t\t\t\t\t\t<a @click=\"nextPage(clickFilter)\" :class=\"{'b-button': true, 'b-button--slider': true, 'b-button--disabled':pageNumber >= resultCount-1 }\" href=\"javascript:void(0);\" title=\"\">\r\n\t\t\t\t\t\t\t\t\t\t<iconAnglerightSvg class=\"b-icon icon-angle-left\"/>\r\n\t\t\t\t\t\t\t\t\t\t</a>\r\n\t\t\t\t\t\t\t</li>\r\n\t\t\t\t\t\t</ul>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"b-number-page\">\r\n\t\t\t\t\t\t<div class=\"b-number-page__text\">\r\n\t\t\t\t\t\t\tПоказать кол-во строк\r\n\t\t\t\t\t\t\t<span></span>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div class=\"b-number-page__text\">\r\n\t\t\t\t\t\t\t<select @change=\"updateListEvent(size)\" style=\"width:40px\" v-model=\"size\">\r\n\t\t\t\t\t\t\t\t<option :value=\"5\">5</option>\r\n\t\t\t\t\t\t\t\t<option :value=\"10\">10</option>\r\n\t\t\t\t\t\t\t\t<option :value=\"20\">20</option>\r\n\t\t\t\t\t\t\t\t<option :value=\"30\">30</option>\r\n\t\t\t\t\t\t\t</select>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nimport iconSvg from \"../../assets/icons/svg/filter.svg\"\r\nimport iconCloseSvg from \"../../assets/icons/svg/close.svg\"\r\nimport iconSearchSvg from \"../../assets/icons/svg/search.svg\"\r\nimport iconAngleleftSvg from \"../../assets/icons/svg/angle-left.svg\"\r\nimport iconAnglerightSvg from \"../../assets/icons/svg/angle-right.svg\"\r\nimport iconDownloadSvg from \"../../assets/icons/svg/download.svg\"\r\nimport iconSortdownSvg from \"../../assets/icons/svg/sort-down.svg\"\r\nimport iconSortupSvg from \"../../assets/icons/svg/sort-up.svg\"\r\nimport orderBy from 'lodash.orderby'\r\n\r\nimport {mapGetters, mapActions} from 'vuex'\r\n\r\nexport default {\r\n\tname: 'listCr',\r\n\tcomponents: { iconSvg, iconSearchSvg, iconCloseSvg, iconDownloadSvg, iconSortdownSvg, iconSortupSvg, iconAngleleftSvg, iconAnglerightSvg },\r\n\tcomputed: {\r\n\t\t...mapGetters({\r\n\t\t\tmedia: 'layout/getMedia',\r\n\t\t\tgetUserInfo: 'auth/getUserInfo',\r\n\t\t\tapiUrl: 'apiUrl'\r\n\t\t}),\r\n\t},\r\n\tdata: () => ({\r\n\t\tallStroke: 0,\r\n\t\tclickFilter: 0,\r\n\t\topen: false,\r\n\t\tcheckColumn: 'number',\r\n\t\tfilter: {\r\n\t\t\tnumber: '',\r\n\t\t\tauthor: '',\r\n\t\t\tnameCR: ''\r\n\t\t},\r\n\t\tcolumns: [\r\n\t\t\t{name: 'Дата изменения', sort: false, nameEng: 'modifiedOn'},\r\n\t\t\t{name: 'Автор изменения, роль', sort: false, nameEng: 'changeAuthorId'},\r\n\t\t\t{name: 'Номер КР', sort: false, nameEng: 'number'},\r\n\t\t\t{name: 'Стадия КР', sort: false, nameEng: 'stageId'},\r\n\t\t\t{name: 'Статус КР', sort: false, nameEng: 'statusId'},\r\n\t\t\t{name: 'Наименование КР', sort: false, nameEng: 'name'}\r\n\t\t],\r\n\t\tlist: [],\r\n\t\tlistFreeze: [],\r\n\t\tpaginatedData: [],\r\n\t\tpageNumber: 0,\r\n\t\tstatuses: null,\r\n\t\tdateFrom: null,\r\n\t\tdateTo: null,\r\n\t\tsize: 30,\r\n\t\tresultCount: 0,\r\n\t\tmkbList: [],\r\n\r\n\t\tstatusesList: [],\r\n\t}),\r\n\tmethods: {\r\n\t\t...mapActions({\r\n\t\t\tapiAll: 'api/apiAll'\r\n\t\t}),\r\n\t\tsortFunc(e) {\r\n\t\t\te.sort = !e.sort\r\n\t\t\tthis.checkColumn = e.nameEng\r\n\t\t\tthis.apiMethods({ columnName: e.nameEng, orderOperation: e.sort ? 'asc' : 'desc' })\r\n\t\t},\r\n\t\tpaginatedDataComputed(list, pageNumber){\r\n\t\t\tlet start = pageNumber * this.size, end = start + this.size;\r\n\t\t\tthis.paginatedData = orderBy(list.slice(start, end), this.checkColumn, 'desc')\r\n\t\t},\r\n\t\tpageCount(list){\r\n\t\t\tlet res = 0;\r\n\t\t\tif(!list) {\r\n\t\t\t\tres = Math.ceil(this.listFreeze.length/this.size)\r\n\t\t\t} else {\r\n\t\t\t\tres = Math.ceil(list.length/this.size)\r\n\t\t\t}\r\n\t\t\tthis.resultCount = res\r\n\t\t},\r\n\t\tnextPage(stage){\r\n\t\t\tif(stage !== 0){\r\n\t\t\t\tthis.pageNumber++;\r\n\t\t\t\tthis.paginatedDataComputed(this.list.filter(f=>f.stage&&f.stage.id == stage), this.pageNumber)\r\n\t\t\t} else {\r\n\t\t\t\tthis.pageNumber++;\r\n\t\t\t\tthis.paginatedDataComputed(this.listFreeze, this.pageNumber)\r\n\t\t\t}\r\n\t\t},\r\n\t\tprevPage(stage){\r\n\t\t\tif(stage !== 0){\r\n\t\t\t\tthis.pageNumber--;\r\n\t\t\t\tthis.paginatedDataComputed(this.list.filter(f=>f.stage&&f.stage.id == stage), this.pageNumber)\r\n\t\t\t} else {\r\n\t\t\t\tthis.pageNumber--;\r\n\t\t\t\tthis.paginatedDataComputed(this.listFreeze, this.pageNumber)\r\n\t\t\t}\r\n\t\t},\r\n\t\tapiMethods(orderByColumn) {\r\n\t\t\tthis.apiAll({type: 'post', url: `api/ProjectHistory`, obj: {\r\n\t\t\t\tstartChangeDate: this.dateFrom ? this.dateFrom : undefined,\r\n\t\t\t\tendChangeDate: this.dateTo ? this.dateTo : undefined,\r\n\t\t\t\tchangeAuthor: this.filter.author ? this.filter.author : undefined,\r\n\t\t\t\tnumber: this.filter.number ? this.filter.number : undefined,\r\n\t\t\t\tname: this.filter.nameCR ? this.filter.nameCR : undefined,\r\n\t\t\t\tstatusId: this.statuses ? this.statuses : undefined,\r\n\t\t\t\torderByColumn: orderByColumn&&orderByColumn.columnName ? orderByColumn : undefined\r\n\t\t\t}, id: undefined})\r\n\t\t\t.then(res => {\r\n\t\t\t\tthis.allStroke = res.data.length\r\n\t\t\t\tthis.list = res.data\r\n\t\t\t\tthis.listFreeze = res.data\r\n\t\t\t\tthis.paginatedDataComputed(this.list, this.pageNumber)\r\n\t\t\t\tthis.pageCount(this.list)\r\n\t\t\t})\r\n\t\t},\r\n\t\tdownloadReports() {\r\n\t\t\tthis.api.post(`${this.apiUrl}api/ProjectHistory/Report`, {\r\n\t\t\t\tstartChangeDate: this.dateFrom ? this.dateFrom : undefined,\r\n\t\t\t\tendChangeDate: this.dateTo ? this.dateTo : undefined,\r\n\t\t\t\tchangeAuthor: this.filter.author ? this.filter.author : undefined,\r\n\t\t\t\tnumber: this.filter.number ? this.filter.number : undefined,\r\n\t\t\t\tname: this.filter.nameCR ? this.filter.nameCR : undefined,\r\n\t\t\t\tstatusId: this.statuses ? this.statuses : undefined,\r\n\t\t\t}, { responseType: 'blob' })\r\n\t\t\t.then((response) => {\r\n\t\t\t\tconst url = window.URL.createObjectURL(new Blob([response.data]));\r\n\t\t\t\tconst link = document.createElement('a');\r\n\t\t\t\tlink.href = url;\r\n\t\t\t\tlink.setAttribute('download', 'Отчет.xlsx');\r\n\t\t\t\tdocument.body.appendChild(link);\r\n\t\t\t\tlink.click();\r\n\t\t\t})\r\n\t\t\t.catch((error) => console.log(error))\r\n\t\t},\r\n\t\tupdateListEvent(size) {\r\n\t\t\tthis.pageNumber = 0\r\n\t\t\tif(this.clickFilter !== 0) {\r\n\t\t\t\tthis.paginatedDataComputed(this.list.filter(f=>f.stage&&f.stage.id == this.clickFilter), this.pageNumber)\r\n\t\t\t\tthis.pageCount(this.list.filter(f=>f.stage&&f.stage.id == this.clickFilter))\r\n\t\t\t} else {\r\n\t\t\t\tthis.paginatedDataComputed(this.listFreeze, 0)\r\n\t\t\t\tthis.pageCount(this.listFreeze)\r\n\t\t\t}\r\n\t\t}\r\n\t},\r\n\tcreated() {\r\n\t\tthis.apiMethods()\r\n\t\tthis.apiAll({type: 'get', url: `api/Statuses`, id: undefined}).then(res => { this.statusesList = res.data })\r\n\t}\r\n}\r\n</script>\r\n\r\n<style>\r\n@import \"../../style/stylePages.css\";\r\n@import \"../../views/ListCr/style/mobile.css\";\r\n\r\ntable tbody tr:hover {\r\n\tbackground: #f1f1f1;\r\n\tbox-shadow: 0 1.5px 6px #00000080;\r\n\tcursor: pointer;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/History"}]}